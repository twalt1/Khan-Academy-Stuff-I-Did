CREATE TABLE Electronics (id INTEGER PRIMARY KEY, name TEXT, brand TEXT, cost REAL, aisle INTEGER);
CREATE TABLE Bread (id INTEGER PRIMARY KEY, name TEXT, cost REAL, aisle INTEGER);

